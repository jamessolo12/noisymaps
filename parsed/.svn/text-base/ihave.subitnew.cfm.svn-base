<cfsetting enablecfoutputonly="true" />
<cfprocessingdirective pageencoding="utf-8" />
<!--- circuit: ihave --->
<!--- fuseaction: subitNew --->
<cftry>
<cfset myFusebox.thisPhase = "appinit">
<cfset myFusebox.thisCircuit = "ihave">
<cfset myFusebox.thisFuseaction = "subitNew">
<cfif myFusebox.applicationStart or
		not myFusebox.getApplication().applicationStarted>
	<cflock name="#application.ApplicationName#_fusebox_#FUSEBOX_APPLICATION_KEY#_appinit" type="exclusive" timeout="30">
		<cfif not myFusebox.getApplication().applicationStarted>
<!--- fuseaction action="system.setup" --->
<cfset myFusebox.thisCircuit = "system">
<cfset myFusebox.thisFuseaction = "setup">
<cftry>
<cfoutput><cfinclude template="../system/model/actSetup.cfm"></cfoutput>
<cfcatch type="missingInclude"><cfif len(cfcatch.MissingFileName) gte 18 and right(cfcatch.MissingFileName,18) is "model/actSetup.cfm">
<cfthrow type="fusebox.missingFuse" message="missing Fuse" detail="You tried to include a fuse model/actSetup.cfm in circuit system which does not exist (from fuseaction system.setup).">
<cfelse><cfrethrow></cfif></cfcatch></cftry>
<cfset myFusebox.thisCircuit = "ihave">
<cfset myFusebox.thisFuseaction = "subitNew">
			<cfset myFusebox.getApplication().applicationStarted = true />
		</cfif>
	</cflock>
</cfif>
<!--- fuseaction action="system.getXfas" --->
<cfset myFusebox.thisPhase = "preprocessFuseactions">
<cfset myFusebox.thisCircuit = "system">
<cfset myFusebox.thisFuseaction = "getXfas">
<cfset xfa.home = "home.welcome" />
<cfset xfa.ineed = "ineed.main" />
<cfset xfa.ihave = "ihave.main" />
<cfset xfa.ineedresults = "ineed.searchResults" />
<cfset xfa.ihavesubmit = "ihave.subitNew" />
<cfset xfa.thanks = "shared.thanks" />
<cfset myFusebox.thisPhase = "requestedFuseaction">
<cfset myFusebox.thisCircuit = "ihave">
<cfset myFusebox.thisFuseaction = "subitNew">
<cftry>
<cfoutput><cfinclude template="../ihave/model/act_iHaveValidate.cfm"></cfoutput>
<cfcatch type="missingInclude"><cfif len(cfcatch.MissingFileName) gte 27 and right(cfcatch.MissingFileName,27) is "model/act_iHaveValidate.cfm">
<cfthrow type="fusebox.missingFuse" message="missing Fuse" detail="You tried to include a fuse model/act_iHaveValidate.cfm in circuit ihave which does not exist (from fuseaction ihave.subitNew).">
<cfelse><cfrethrow></cfif></cfcatch></cftry>
<cfif variables.bValid eq true>
<cftry>
<cfoutput><cfinclude template="../ihave/model/act_InsertArtefact.cfm"></cfoutput>
<cfcatch type="missingInclude"><cfif len(cfcatch.MissingFileName) gte 28 and right(cfcatch.MissingFileName,28) is "model/act_InsertArtefact.cfm">
<cfthrow type="fusebox.missingFuse" message="missing Fuse" detail="You tried to include a fuse model/act_InsertArtefact.cfm in circuit ihave which does not exist (from fuseaction ihave.subitNew).">
<cfelse><cfrethrow></cfif></cfcatch></cftry>
<cfset application.coldSpring.objcfml.cfLocation(url="#myFusebox.getMyself()##xfa.thanks#", addtoken="false")>
<cfset application.Coldspring.objCfml.cfAbort()>
<cfelse>
</cfif>
<cfcatch><cfrethrow></cfcatch>
</cftry>

