<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE circuit>
<circuit access="public" xmlns:cf="cf/">

	<fuseaction name="main">
		<include template="model/act_iNeed" />
		<include template="display/dsp_iNeed" contentvariable="strContent"/>
		<do action="shared.mainLayout" />
	</fuseaction>

	<fuseaction name="searchResults">
		<include template="model/act_iNeedResults" />
		<include template="display/dsp_results" contentvariable="strContent"/>
		<do action="shared.mapLayout" />
	</fuseaction>
</circuit>
